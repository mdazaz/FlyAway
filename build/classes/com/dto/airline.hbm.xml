<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dto.Airline" table="AIRLINES">
        <id column="ID" name="id">
        <generator class="increment" ></generator>
        </id>
         <property column="NAME" name="name" type="java.lang.String" not-null="true" unique="true" />        
         <property column="LOGO" name="logo" type="java.lang.String" />
          <property column="STATUS" name="status" type="java.lang.Integer" />
    </class>
</hibernate-mapping>